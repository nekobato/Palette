{
  "name": "palette",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "ws": "node-dev ./ws/index.js",
    "start": "node ws/index.js"
  },
  "dependencies": {
    "qrcode": "1.2.0",
    "socket.io": "2.0.4",
    "vue": "2.5.13",
    "vue-router": "3.0.1",
    "vuex": "3.0.1",
    "ws": "4.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.0.0-beta.2",
    "@vue/cli-plugin-eslint": "3.0.0-beta.2",
    "@vue/cli-service": "3.0.0-beta.2",
    "node-dev": "3.1.3",
    "paper": "0.11.5",
    "socket.io-client": "2.0.4",
    "vue-template-compiler": "2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
